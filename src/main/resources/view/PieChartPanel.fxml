<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Label?>
<?import javafx.scene.image.Image?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.VBox?>

<VBox alignment="CENTER" xmlns="http://javafx.com/javafx/18" xmlns:fx="http://javafx.com/fxml/1">
   <children>
      <Label fx:id="pieChartTitle"  text="\$pieChartTitle" styleClass="label-header"  />

      <ImageView fitHeight="300.0" fitWidth="300.0" preserveRatio="true">
         <image>
            <Image url="@../images/pie_chart.png" />
         </image>
      </ImageView>
   </children>
</VBox>
